not : Bool -- Bool
    =: [
    { case
        | True -> False
        | _    -> True
    }
]

odd : Int -- Bool
    =: [ dup 0 =
            if( drop False
              , - 1 even )
       ]

even : Int -- Bool
     =: [ dup 0 =
            if( drop True
              , - 1 odd )
        ]

%foreign cabi even
